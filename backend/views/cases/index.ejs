<div class="container mt-5">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">My Cases</h3>
      </div>
      <div class="panel-body">
        <table class="table table-striped table-hover">
            <tr>
              <th>Name</th>
              <th>Subject</th>
              <th>Mobile No.</th>
              <th>Status</th>
              <th>Created on</th>
            </tr>
          <% allCases.forEach(function(acase){ %>
            <tr>
              <td><%=acase.user.name%></td>
              <td><%=acase.subject%></td>
              <td><%=acase.mobileNo%></td>
              <td><%=acase.status%></td>
              <td><%=acase.created.toDateString()%></td>

            </tr>
          <% }); %>
          </table>
      </div>
    </div>
</div>